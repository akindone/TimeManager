<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:gravity="center_vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_eic_color"
        android:layout_width="@dimen/icon_size_5xs"
        android:layout_height="@dimen/icon_size_5xs"
        android:background="@color/ad_bill"
        android:layout_margin="@dimen/margin_3xs"/>
    <TextView
        android:id="@+id/label_expand_child"
        android:layout_width="@dimen/size_0"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:layout_height="@dimen/height_button_small"
        android:textSize="@dimen/font_small"
        android:layout_margin="@dimen/margin_3xs"/>
    <TextView
        android:id="@+id/tv_eic_default"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="默认"
        android:layout_margin="@dimen/margin_3xs"
        android:textSize="@dimen/font_4xs" />

</LinearLayout>